#!/usr/bin/env bash
# Find the PID (Process ID) of the running 7-highlander process
pid=$(pgrep 7-highlander)

# Check if the process exists
if [ -z "$pid" ]; then
    echo "No 7-highlander process found."
else
    # Kill the process
    kill "$pid"
    echo "7-highlander process with PID $pid has been killed."
fi

